Function FindEnemyPic()
    Dim enemyPicName
    enemyPicName = Array("enemy0.bmp", "enemy1.bmp", "enemy2.bmp")	
    For Each enemyPic In enemyPicName
        FindPic gL + 120, gT + 130, gL + 980, gT + 620, "Attachment:\" & enemyPic, 0.1, intX, intY
        If intX > 0 And intY > 0 Then 
            FindEnemyPic = intX & "|" & intY
            Exit Function
        End If
    Next
    FindEnemyPic = ""
End Function

