Sub ©Л╫щ╢Р©╙╫ГцФ(x)  //xн╙©Л╫щ╪Э╣двжд╦
	KeyDown "Alt", 1
	Delay 300
	Select Case x
	Case "Q"
		KeyPress "Q", 1
		Delay 500
	Case "E"
		KeyPress "E", 1
		Delay 500
	Case "F"
		KeyPress "F", 1
		Delay 500
	Case "K"
		KeyPress "K", 1
		Delay 500
	Case "B"
		KeyPress "B", 1
		Delay 500
	Case "P"
		KeyPress "P", 1
		Delay 500
	Case "C"
		KeyPress "C", 1
		Delay 500
	Case "S"
		KeyPress "S", 1
		Delay 500
	Case "T"
		KeyPress "T", 1
		Delay 500
	End Select
	KeyUp "Alt", 1
	Delay 1000
	
End Sub
Sub ╥╤н╖я║тЯд©╠Й(x, y, m, n, t, z)//x,yн╙╥╤н╖ЁУй╪вЬ╠Йё╛m,nйг╥╤н╖╫АйЬвЬ╠Йё╛й╧сцQQ╫ьм╪╩Я╣цё╛zн╙м╪ф╛б╥╬╤ё╛юЩхГ"Attachment:\жМ2.bmp"ё╗╠ёаТк╚рЩ╨её╘,tн╙въ╣╫╦цNPCкЫпХ╣дй╠╪Д()
								 //ййсцтзЁ║╬╟╡╩╩А╦д╠ДвжлЕяуи╚╣дгИ©Жё╛пХр╙╤тм╪ф╛╫Ьпп╢╕юМё╛╢С╪рпХя╖о╟╪╦╡╫ps
	For i = 1 To t	
		//FindPic 0, 0, 1920, 1080, "Attachment:\жМ2.bmp", 0.8, intX, intY
		FindPic x,y,m,n,z,0.85,intX,intY
		If intX > 0 And intY > 0 Then 

			MoveTo intX , intY 
			Delay 1000
			LeftClick 1
		End If	
	Next
End Sub

