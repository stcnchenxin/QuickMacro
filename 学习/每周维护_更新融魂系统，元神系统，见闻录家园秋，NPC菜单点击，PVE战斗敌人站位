Sub я╟б╥(x, y,time)
    Delay 1000
    SayString "$run_to " & x & "," & y& ""
    KeyPress "Enter", 1
    Delay time*1000
End Sub
Sub ур╣ЦNPC(x, y, time,weizhi)  'хЩ╦Ж╡нйЩ╥ж╠Пн╙NPC╣дx║╒yвЬ╠Йё╛рт╪╟т╓╪фпХр╙╣дя╟б╥й╠╪Дё╗sё╘ё╛н╩жцтзNPC╣диообвСср
    'се╩╞я╟б╥вЬ╠Й╣Цё╛╡╩дэ╨мNPCжь╣Ч 
    Dim x1, y1, p1
    y1 = 540
    x1 = 960
    If (weizhi=1)
        y =y+5
        y1=y1+80
    End If
    If (weizhi=2)
        y = y - 5
        y1=y1-150
    End If
    If (weizhi=3)
        x = x-5
        x1= x1+100
    End If
    If (weizhi=4)
        x =x+5
        x1= x1-100
    End If
    'йДхКя╟б╥ж╦аН╡╒╩ьЁ╣ё╛ясЁыя╟б╥й╠╪Д
    SayString "$run_to " & x & "," & y& ""
    KeyPress "Enter", 1
    Delay time * 1000
    'я╟б╥╫АйЬ
    'пчуЩ╣ьм╪╠ъ╫ГNPC╣Ц╩ВвЬ╠Й
    If (x < 60) Then 
        p1 = 60 - x
        x1 = x1 - 16 * p1
        Goto ж╢ппy
    End If
    If (changjing_max_x-x < 60) Then  
        p1 = 60 -(changjing_max_x-x)
        x1=x1+16*p1
    End If
    Rem ж╢ппy
    If (y < 34) Then 
        p1 = 34 - y
        y1 = y1 + 16* p1
        Goto ж╢пп
    End If
    If (changjing_max_y-y< 34) Then 
        p1 = 34 -(changjing_max_y-y)
        y1 = y1 - 16 * p1
    End If
    Rem ж╢пп
    Delay 1000
    MoveTo x1, y1-70
    Delay 1000
    LeftClick 1
    x1 =960
    y1=540
End Sub
Sub NPC╡к╣╔╣Ц╩В(menu_num,click_num)  '╣Ц╩В╪╦╨е╡к╣╔ё╛х╩╨С╣Ц╩ВйС╠Й╪╦╢н
	menu = menu_num
    
    For i = 1 To 8
    	If menu > 3 Then 
    		menu = menu - 3 
    		MoveTo 1252, 1057
    		Delay 300
    		LeftClick 1
    		Delay 300
    		Else 
    			Exit For
    	End If
    Next
    MoveTo x_menu1, y_menu1 + (menu - 1) * 20
    
    '╣Ц╩ВясЁы╠ё╩╓
    Delay 1000
    '╤т╩╟╨С╣д╣Ц╩Вя╜╩╥
    Rem ╣Ц╩ВйС╠Й
    If(click_num>0)
        LeftClick 1
        Delay 1000
        click_num = click_num - 1
        Goto ╣Ц╩ВйС╠Й
        Delay 1000
    End If
End Sub
Sub ╧ь╠усно╥
    KeyDown "Alt", 1
    KeyDown "F4", 1
    KeyUp "Alt", 1
    KeyUp "F4",1
End Sub
Function урЁ║╬╟╠Ю╨е(Place)
	//╢Р©╙Excelнд╣╣╧╓вВ╠М
	//Call Plugin.Office.OpenXls("D:/╫е╠╬нд╣╣/Ё║╬╟.xls")
	/*
	//оРExcel╧╓вВ╠Мё╗1ё╘╣зр╩ппп╢хКйЩ╬щ
	Call Plugin.Office.WriteXls(1, 1, 1, "1тбмЁ╪ф")
	Call Plugin.Office.WriteXls(1, 1, 2, "958")
	Call Plugin.Office.WriteXls(1, 1, 3, "╫П╤Н1")
	Call Plugin.Office.WriteXls(1, 1, 4, "9878.34")
	
	//оРExcel╧╓вВ╠Мё╗1ё╘╣з╤Чппп╢хКйЩ╬щ
	Call Plugin.Office.WriteXls(1, 2, 1, "1тбмЁ╪ф")
	Call Plugin.Office.WriteXls(1, 2, 2, "852")
	Call Plugin.Office.WriteXls(1, 2, 3, "╫П╤Н2")
	Call Plugin.Office.WriteXls(1, 2, 4, "8934.04")
	
	//╤ах║Excel╧╓вВ╠М(1)╣зр╩пп╣з1║╒4╦Ядзхщ
	Text1 = Plugin.Office.ReadXls(1, 1, 1)
	Text2 = Plugin.Office.ReadXls(1, 1, 4)
	
	For i = 1 to 20000
		Text1 = Plugin.Office.ReadXls(1, i, 2)
		If(Text1 = Place) Then
			Text2 = Plugin.Office.ReadXls(1, i, 1)
			//MessageBox Text2 & "╢Рс║" & Text1
			Exit For
		End If
	Next
	
		
	//╧ь╠уExcelнд╣╣╧╓вВ╠М
	Call Plugin.Office.CloseXls()
	*/
	For i = 0 To 1999
		Text1 = Places(i)(1)
		If (Text1 = Place) Then 
			Text2 = Places(i)(0)
			Exit For
		End If
	Next
	
	урЁ║╬╟╠Ю╨е = Text2
End Function
Function ураИйч╠Ю╨е(ID)
	//╢Р©╙Excelнд╣╣╧╓вВ╠М
	//Call Plugin.Office.OpenXls("D:/╫е╠╬нд╣╣/Ё║╬╟.xls")
	/*
	//оРExcel╧╓вВ╠Мё╗1ё╘╣зр╩ппп╢хКйЩ╬щ
	Call Plugin.Office.WriteXls(1, 1, 1, "1тбмЁ╪ф")
	Call Plugin.Office.WriteXls(1, 1, 2, "958")
	Call Plugin.Office.WriteXls(1, 1, 3, "╫П╤Н1")
	Call Plugin.Office.WriteXls(1, 1, 4, "9878.34")
	
	//оРExcel╧╓вВ╠Мё╗1ё╘╣з╤Чппп╢хКйЩ╬щ
	Call Plugin.Office.WriteXls(1, 2, 1, "1тбмЁ╪ф")
	Call Plugin.Office.WriteXls(1, 2, 2, "852")
	Call Plugin.Office.WriteXls(1, 2, 3, "╫П╤Н2")
	Call Plugin.Office.WriteXls(1, 2, 4, "8934.04")
	
	//╤ах║Excel╧╓вВ╠М(1)╣зр╩пп╣з1║╒4╦Ядзхщ
	Text1 = Plugin.Office.ReadXls(1, 1, 1)
	Text2 = Plugin.Office.ReadXls(1, 1, 4)
	
	For i = 1 to 20000
		Text1 = Plugin.Office.ReadXls(1, i, 2)
		If(Text1 = Place) Then
			Text2 = Plugin.Office.ReadXls(1, i, 1)
			//MessageBox Text2 & "╢Рс║" & Text1
			Exit For
		End If
	Next
	
		
	//╧ь╠уExcelнд╣╣╧╓вВ╠М
	Call Plugin.Office.CloseXls()
	*/
	For i = 0 To 1999
		Text1 = Summon(i)(1)
		If (Text1 = ID) Then 
			Text2 = Summon(i)(0)
			Exit For
		End If
	Next
	
	ураИйч╠Ю╨е = Text2
End Function
Sub ©╙й╪мФ╥╗╡Бйт()
    Call ╢Рж╦аН("$clear_bag")
    Call ╢Рж╦аН("$cm")
    Call ╢Рж╦аН("$lf")
    Call ╢Рж╦аН("$setprop selfid,exp,0")
    Call ╟╢╪Эел("Esc", 4)
    Call ╢Рж╦аН("$das")
    Call ╢Рж╦аН("$setprop selfid,grade,10")
    Call ╢Рж╦аН("$setprop selfid,CurMp,200")
    Call ╢Рж╦аН("$setprop selfid,CurHp,200")
    Call ╢Рж╦аН("$setprop selfid,GenGuPoint,40")
    Call ╢Рж╦аН("$setprop selfid,cash,0")
    Call ╢Рж╦аН("$setprop selfid,bind_cash,0")
    Call ╢Рж╦аН("$sl 4")
    //Call ╢Рж╦аН("$get_info end")
End Sub
Sub ЁУй╪аИйч()
	Call ╢Рж╦аН("$initsummon 20002")
	Call ©Л╫щ╢Р©╙╫ГцФ("P")
	Call я║тЯаИйч(1)
	Call аИйчиХжц╡ну╫()
	Call аИйчиЩ╪╤╡╒вт╤╞╪смф╪Ж╣Ц()
	Call ╢Рж╦аН("$SetSummonProp right,hp,300")
	Call ╢Рж╦аН("$SetSummonProp right,mp,600")
	KeyPress "Esc", 4
	Delay 300
End Sub
Sub ╦Жпт╩╞╫е╠╬©╙й╪(Bag, Mission, Grade, Money, Hp, Gengu, Mp)  //bagн╙1тРгЕ©у╠Ё╟Эн╙0тР╡╩╠Дё╛missionн╙1тРгЕ©ухннЯн╙0тР╡╩╠Дё╛gradeн╙хкнО╣х╪╤ё╛moneyн╙хкнО╫Пг╝ё╛hp║╒mp╡╩╫Бймё╛GenguйгхкнОвН╢Ся╙а©
	KeyPress "Esc", 4
	Delay 300
	Call ╢Рж╦аН("$setprop selfid,exp,0")
	Delay 300
	Call ╢Рж╦аН("$sl 4")
	Delay 300
	If Bag = 1 Then 
		Call ╢Рж╦аН("$clear_bag")
	End If
	If Mission = 1 Then 
		Call ╢Рж╦аН("$cm")
	End If
	If Grade > 0 Then 
		Call ╢Рж╦аН("$setprop selfid,grade," & Grade)
		ElseIf Grade = 0 Then
		Call ╢Рж╦аН("$setprop selfid,grade,1")
	End If
	Call ╢Рж╦аН("$setprop selfid,cash," & Money)
	Call ╢Рж╦аН("$setprop selfid,bind_cash," & Money)
	If Gengu > 0 Then 
		Call ╢Рж╦аН("$setprop selfid,GenGuPoint," & Gengu)
		ElseIf Gengu = 0 Then
		Call ╢Рж╦аН("$setprop selfid,GenGuPoint,1")
	End If
	If Hp > 0 Then 
		Call ╢Рж╦аН("$setprop selfid,CurHp," & Hp)
		ElseIf Hp = 0 Then
		Call ╢Рж╦аН("$setprop selfid,CurHp,1")
	End If
	If Mp > 0 Then 
		Call ╢Рж╦аН("$setprop selfid,CurMp," & Mp)
		ElseIf Mp = 0 Then
		Call ╢Рж╦аН("$setprop selfid,CurMp,1")
	End If
	
End Sub
Sub ╦Жпт╩╞╩Я╣цаИйч(Summon, Grade, Point, Hp, Mp)		//Gradeн╙аИйч╣х╪╤ё╛pointн╙йг╥Я╪с╣Цё╛hpн╙аИйчфЬя╙ё╗0н╙д╛хож╣ё╘ё╛mpн╙аИйч╥╗а╕ё╗0н╙д╛хож╣ё╘,summonн╙аИйчцШЁф
	LingShou = ураИйч╠Ю╨е(Summon)
	Call ╢Рж╦аН("$initsummon " & LingShou)
	Call ©Л╫щ╢Р©╙╫ГцФ("P")
	Call я║тЯаИйч(1)
	Call аИйчиХжц╡ну╫()
	Call ╢Рж╦аН("$ssp right, grade, " & Grade)
	If Point = 1 Then 
		MoveTo 455, 735
		LeftClick 1
		Delay 500
		MoveTo 510, 735
		LeftClick 1
		Delay 500
	End If
	If Hp > 0 Then 
		Call ╢Рж╦аН("$SetSummonProp right,hp," & Hp)
	End If
	If Mp > 0 Then 
		Call ╢Рж╦аН("$SetSummonProp right,mp," & Mp)
	End If
	
End Sub

Sub ╟╢╪Эел(x, num)
    Rem ╟╢╪Эел╢нйЩ
    if(num>0)
        KeyPress x, 1
        Delay 100
        num = num - 1
        Goto ╟╢╪Эел╢нйЩ
    End If
End Sub
Sub ╢Рж╦аН(x)
