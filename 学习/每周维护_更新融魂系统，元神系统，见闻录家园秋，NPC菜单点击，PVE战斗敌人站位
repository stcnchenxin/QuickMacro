'м╗сц╥╫╥╗╤╗рЕ*********************************************************************************************************************************************

Sub д╛хонОф╥ю╦╣Ц╩В(x,y,click_type, num)
    MoveTo wupinlan_jizhundian_x + (y - 1) * wupinlan_xchange, wupinlan_jizhundian_y + (x - 1) * wupinlan_ychange
    Delay 1000
    If (click_type = 1) Then 
        Rem ╤Ю╢н╣ЦйС╠ЙвС╪Э
        If (num>0)
            LeftClick 1
            num = num - 1
            Delay 1000
            goto ╤Ю╢н╣ЦйС╠ЙвС╪Э
        End If
    End If
    If (click_type = 2) Then 
        Rem ╤Ю╢н╣ЦйС╠Йср╪Э
        If (num>0)
            RightClick 1
            num = num - 1
            Delay 1000
            goto ╤Ю╢н╣ЦйС╠Йср╪Э
        End If		
    End If	
End Sub 
Sub ╥гд╛хонОф╥ю╦╣Ц╩В(x_coordinate,y_coordinate,x,y,click_type, num)
    MoveTo x_coordinate + (y - 1) * wupinlan_xchange, y_coordinate + (x - 1) * wupinlan_ychange
    Delay 1000
    If (click_type = 1) Then 
        Rem ╤Ю╢н╣ЦйС╠ЙвС╪Э
        If (num>0)
            LeftClick 1
            num = num - 1
            Delay 1000
            goto ╤Ю╢н╣ЦйС╠ЙвС╪Э
        End If
    End If
    If (click_type = 2) Then 
        Rem ╤Ю╢н╣ЦйС╠Йср╪Э
        If (num>0)
            RightClick 1
            num = num - 1
            Delay 1000
            goto ╤Ю╢н╣ЦйС╠Йср╪Э
        End If		
    End If
End Sub
Sub ╫ьм╪╣ЦйС╠Й(x, y,click_type,num)  'xyвЬ╠Йё╛1н╙вС╪Э2н╙ср╪Эё╛numн╙╣Ц╩В╢нйЩ
    MoveTo x, y
    If (click_type = 1) Then 
        Rem ╤Ю╢н╣ЦйС╠ЙвС╪Э
        If (num>0)
            LeftClick 1
            num = num - 1
            Delay 1000
            goto ╤Ю╢н╣ЦйС╠ЙвС╪Э
        End If
    End If
    If (click_type = 2) Then 
        Rem ╤Ю╢н╣ЦйС╠Йср╪Э
        If (num>0)
            RightClick 1
            num = num - 1
            Delay 1000
            goto ╤Ю╢н╣ЦйС╠Йср╪Э
        End If		
    End If	
End Sub
Sub ╣ьцШ╫ЬхКЁ║╬╟(changjing)  //changjingюОп╢хКжпнд╣ь╣ЦцШ
	
	For i = 0 to 1999
		Text1 = Places(i)(1)
		If(Text1 = changjing) Then
			Text2 = Places(i)(2)
			Text3 = Places(i)(3)
			//MessageBox Text2 & "╢Рс║" & Text1
			Exit For
		End If
	Next
	
	changjing_max_x = Text2/16
	changjing_max_y = Text3/16
End Sub
Sub йДЁЖlog(XinXi)		//XinXiн╙йДхКоКр╙йДЁЖ╣дlogюЮпмё╛юЩхГё╛хкнО║╒аИйч║╒нОф╥║╒жВоъхннЯ
	Select Case XinXi
		Case "хкнО"
			info = "exp"
		Case "аИйч"
			info = "summon"
		Case "нОф╥"
			info = "item"
		Case "жВоъхннЯ"
			info = "misson"
	End Select
	text = "$get_info " & info
	Call ╢Рж╦аН(text)
End Sub
