Sub й╧сцаИйчнОф╥(x, y)   //xн╙пп,yн╙апё╛нОф╥ю╦ппап╨е
	If(y < 7) Then
	MoveTo 615 + (y - 1) * 40, 335 + (x - 1) * 40
	Delay 400
	RightClick 1
	Delay 400
	ElseIf (y >= 7) Then
		x = x + 1
		y = y - 6
		MoveTo 615 + (y - 1) * 42, 335 + (x - 1) * 42
		Delay 400
		RightClick 1
		Delay 400
	End If
End Sub
