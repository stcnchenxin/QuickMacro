'optimize Game Obeject 勾选确认的找图区域
Const OGO_FINDPIC_POS_X1 = 1340
Const OGO_FINDPIC_POS_Y1 = 205
Const OGO_FINDPIC_POS_X2 = 1505
Const OGO_FINDPIC_POS_Y2 = 260


'整合区域的找图区域
Const ETtE_FINDPIC_POS_X1 = 1330
Const ETtE_FINDPIC_POS_Y1 = 240
Const ETtE_FINDPIC_POS_X2 = 1595
Const ETtE_FINDPIC_POS_Y2 = 505

' 骨骼坐标信息 
Const GUGE_POS_X = 1110
Const GUGE_POS_Y = 150

'滚动条坐标信息
Const GUNDONGTIAO_POS_X = 1042
Const GUNDONGTIAO_POS_Y = 188

Do
    '移动到骨骼文件上并点击
    Call MoveAndClick(GUGE_POS_X, GUGE_POS_Y, 500, 800)
    
    '判断是否有勾选
    Dim isCheck
    isCheck = 1
    If Not FindOptiGameObjPic() Then 
        TracePrint "没有勾选：" & GetSelectFileName()
        isCheck = 0
    End If
    
    '如果有勾选则检查内容，有重复的骨骼就打印并退出循环
    If isCheck Then
        Dim ettePicArr
        ettePicArr = Array("BIP001_Spine1", "BIP001_Head", "BIP001_L_Clavicle", "BIP001_L_Foot", "BIP001_L_Hand", "BIP001_Pelvis", "BIP001_R_Clavicle", "BIP001_R_Foot", "BIP001_R_Hand", "Empty")
        For Each ettePic In ettePicArr
            If FindSamePicInArea(ETtE_FINDPIC_POS_X1, ETtE_FINDPIC_POS_Y1, ETtE_FINDPIC_POS_X2, ETtE_FINDPIC_POS_Y2, "Attachment:\" & ettePic & ".bmp", 0.95) = 2 Then 
                TracePrint "有重复的骨骼：" & GetSelectFileName()
                Exit For
            End If
        Next
    End If
    
    Call MoveAndClick(GUNDONGTIAO_POS_X, GUNDONGTIAO_POS_Y, 500, 500)
    KeyPress "Down", 1
    Delay 500
Loop

Sub MoveAndClick(x, y, moveDealy, clickDelay)
	MoveTo x, y
	Delay moveDealy
	LeftClick 1
	Delay clickDelay
End Sub

Function GetSelectFileName()
    Delay 200
    KeyPress "F2", 1
    Delay 500
    KeyDown 17, 1
    KeyPress 67, 1
    KeyUp 17, 1
    Delay 500
    GetSelectFileName = Plugin.Sys.GetCLB 
End Function

Function FindOptiGameObjPic()
    If (IsHasPic(OGO_FINDPIC_POS_X1, OGO_FINDPIC_POS_Y1, OGO_FINDPIC_POS_X2, OGO_FINDPIC_POS_Y2, "Attachment:\OGO勾选.bmp", 0.9)) Then
        FindOptiGameObjPic = True
    Else 
        FindOptiGameObjPic = False
    End If
End Function

Function FindSamePic(x1, y1, x2, y2, picPath, picSim)
    FindPic x1, y1, x2, y2, picPath, picSim, intX, intY
    If intX > 0 And intY > 0 Then 
        y1 = intY + 10
    End If
    If IsHasPic(x1, y1, x2, y2, picPath, picSim) Then 
        FindSamePic = True
    Else 
    	FindSamePic = False
    End If
End Function

Function IsHasPic(x1, y1, x2, y2, picPath, picSim)
    FindPic x1, y1, x2, y2, picPath, picSim, intX, intY
    If intX > 0 And intY > 0 Then 
        IsHasPic = True
    Else 
        IsHasPic = False
    End If
End Function

Function FindSamePicInArea(x1, y1, x2, y2, picPath, picSim)
    If (IsHasPic(x1, y1, x2, y2, picPath, picSim)) Then 
        FindSamePicInArea = 1
        If FindSamePic(x1, y1, x2, y2, picPath, picSim) Then 
            FindSamePicInArea = 2
        End If
    Else 
        FindSamePicInArea = 0
    End If
End Function
