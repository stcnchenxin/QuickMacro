Event Form1.ProductRpcButton.Click
    Dim rpcFilePath, needTab
    rpcFilePath = "D:\rpc\q6rpc.lua"
    needTab = False
	
    Call Plugin.File.DeleteFile(rpcFilePath)
    Call WriteInRpcTitle(rpcFilePath)
	
    If Form1.LoopInputBox0.Text <> "" Then 
        Call WriteInTxt(rpcFilePath, "for i = 1, " & Int(Form1.LoopInputBox0.Text) & " do")
        needTab = True
    End If    
    If Form1.RpcLabel1.Caption <> "" Then
        Call WriteInRpcPram(rpcFilePath, Form1.LoopInputBox1.Text, Form1.RpcLabel1.Caption, Form1.ParmInputBox1.Text, needTab)
    End If

    If Form1.RpcLabel2.Caption <> "" Then
        Call WriteInRpcPram(rpcFilePath, Form1.LoopInputBox2.Text, Form1.RpcLabel2.Caption, Form1.ParmInputBox2.Text, needTab)
    End If
    
    If Form1.RpcLabel3.Caption <> "" Then
        Call WriteInRpcPram(rpcFilePath, Form1.LoopInputBox3.Text, Form1.RpcLabel3.Caption, Form1.ParmInputBox3.Text, needTab)
    End If
    
    If Form1.RpcLabel4.Caption <> "" Then
        Call WriteInRpcPram(rpcFilePath, Form1.LoopInputBox4.Text, Form1.RpcLabel4.Caption, Form1.ParmInputBox4.Text, needTab)
    End If
    
    If Form1.RpcLabel5.Caption <> "" Then
        Call WriteInRpcPram(rpcFilePath, Form1.LoopInputBox5.Text, Form1.RpcLabel5.Caption, Form1.ParmInputBox5.Text, needTab)
    End If
    
    If Form1.RpcLabel6.Caption <> "" Then
        Call WriteInRpcPram(rpcFilePath, Form1.LoopInputBox6.Text, Form1.RpcLabel6.Caption, Form1.ParmInputBox6.Text, needTab)
    End If
        
    If Form1.LoopInputBox0.Text <> "" Then 
        Call WriteInTxt(rpcFilePath, "end")
    End If  
        
    Call Plugin.Msg.Tips("Éú³ÉÍê±Ï£¡")
End Event

'½«Ð­ÒéÎÄ¼þµÄ¿ªÍ·º¯ÊýÐ´Èë¶ÔÓ¦luaÎÄ¼þ£¬²ÎÊý¡¾Ð´ÈëµÄluaÎÄ¼þÂ·¾¶¡¿
Sub WriteInRpcTitle(writeInFilePath)
    Dim rpcTitlePath, rpcTitleArr, text
    rpcTitlePath = "C:\Q6×Ô¶¯²âÊÔÊý¾Ý\Ð­Òé¸ü»»\rpc_title.txt"
	
    rpcTitleArr = Split(ReadFormTxt(rpcTitlePath), "|")
    For Each text In rpcTitleArr
        Call WriteInTxt(writeInFilePath, text)
    Next
    Call WriteInTxt(writeInFilePath, Chr(10))
End Sub

'½«Ð­ÒéµÄÄÚÈÝÐ´Èë¶ÔÓ¦luaÎÄ¼þ£¬²ÎÊý£º¡¾Ð´ÈëµÄluaÎÄ¼þÂ·¾¶£¬Ð­ÒéÑ­»·´ÎÊý£¬Ð­ÒéËùÊôÏµÍ³£¬Ð­ÒéµÄ²ÎÊý£¬ÊÇ·ñÐèÒªÔÚ×î¿ªÍ·¼Ótab£¨Èç¹ûÓÐ×ÜÑ­»·¾ÍÐèÒªÔÙ¼Ó¸ötab£©¡¿
Sub WriteInRpcPram(writeInFilePath, loopTimes, rpcName, rpcPram, needTab)
    If loopTimes = "" Then 
        If needTab Then
            Call WriteInTxt(writeInFilePath, Chr(9) & "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
        Else 
            Call WriteInTxt(writeInFilePath, "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
        End IF
    Else 
        loopTimes = Int(loopTimes)
        If needTab Then
            Call WriteInTxt(writeInFilePath, Chr(9) & "for i = 1, " & loopTimes & " do")
            Call WriteInTxt(writeInFilePath, Chr(9) & Chr(9) & "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
            Call WriteInTxt(writeInFilePath, Chr(9) & "end")
        Else 
            Call WriteInTxt(writeInFilePath, "for i = 1, " & loopTimes & " do")
            Call WriteInTxt(writeInFilePath, Chr(9) & "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
            Call WriteInTxt(writeInFilePath, "end")
        End If
        
    End If
End Sub


