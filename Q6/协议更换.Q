'根据协议的功能找到对应的协议内容并显示在第3列的标签上
Event Form1.FunctionComboBox1.SelectChange
    If Form1.FunctionComboBox1.ListIndex < 0 Then 
        Exit Event
    End If

    Dim rpcSystem, rpcFunction, systemListArr, functionListArr, rpcFolder, rpcContent, rpcContentArr, i
    rpcFolder = "C:\Q6自动测试数据\协议更换\RpcFolder\"
	
    '获取第一个系统的系统名
    systemListArr = Split(Form1.SystemComboBox1.List, "|")	
    rpcSystem = systemListArr(Form1.SystemComboBox1.ListIndex)
	
    '获取第二个功能的功能名
    functionListArr = Split(Form1.FunctionComboBox1.List, "|")
    rpcFunction = functionListArr(Form1.FunctionComboBox1.ListIndex)
		
    '通过功能找到对应目录
    rpcContent = ReadFormTxt(rpcFolder & rpcSystem & ".txt")
    rpcContentArr = Split(rpcContent, "|")
    
    For i = 0 To UBound(rpcContentArr) - 1 Step 4
        If rpcFunction = rpcContentArr(i) Then 
