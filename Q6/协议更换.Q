'将协议文件的开头函数写入对应lua文件，参数【写入的lua文件路径】
Sub WriteInRpcTitle(writeInFilePath)
    Dim rpcTitlePath, rpcTitleArr, text
    rpcTitlePath = "C:\Q6自动测试数据\协议更换\rpc_title.txt"
	
    rpcTitleArr = Split(ReadFormTxt(rpcTitlePath), "|")
    For Each text In rpcTitleArr
        Call WriteInTxt(writeInFilePath, text)
    Next
    Call WriteInTxt(writeInFilePath, Chr(10))
End Sub

'将协议的内容写入对应lua文件，参数：【写入的lua文件路径，协议循环次数，协议所属系统，协议的参数，是否需要在最开头加tab（如果有总循环就需要再加个tab）】
Sub WriteInRpcPram(writeInFilePath, loopTimes, rpcName, rpcPram, needTab)
    If loopTimes = "" Then 
        If needTab Then
            Call WriteInTxt(writeInFilePath, Chr(9) & "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
        Else 
            Call WriteInTxt(writeInFilePath, "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
        End IF
    Else 
        loopTimes = Int(loopTimes)
        If needTab Then
            Call WriteInTxt(writeInFilePath, Chr(9) & "for i = 1, " & loopTimes & " do")
            Call WriteInTxt(writeInFilePath, Chr(9) & Chr(9) & "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
            Call WriteInTxt(writeInFilePath, Chr(9) & "end")
        Else 
            Call WriteInTxt(writeInFilePath, "for i = 1, " & loopTimes & " do")
            Call WriteInTxt(writeInFilePath, Chr(9) & "xpcall(send, __G__TRACKBACK__, " & Chr(34) & rpcName & Chr(34) & ", " & rpcPram & ")")
            Call WriteInTxt(writeInFilePath, "end")
        End If
        
    End If
End Sub


