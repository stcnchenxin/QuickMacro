//´ÓtxtÎÄ±¾¶ÁÈ¡Êý¾ÝÖ®ºó×îºóÒ»¸öÔªËØ»áÎª¿Õ£¬Õâ¸öº¯Êý»á½«ËüÏû³ýµô£¬²ÎÊý£º¡¾ÐèÒª´¦ÀíµÄ×Ö·û´®£¬·Ö¸î·û¡¿
Function GetTxtWithoutLastSplit(str, splitStr)
    Dim i, textArr
    textArr = Split(str, splitStr)
    GetTxtWithoutLastSplit = textArr(0)
    If GetTxtWithoutLastSplit = "" Then 
        TracePrint "´«½øÀ´µÄ×Ö·û´®µÚÒ»¸öÔªËØÎª¿Õ£¬Çë¼ì²é"
    End If
    For i = 1 To Ubound(textArr) - 1
        GetTxtWithoutLastSplit =  GetTxtWithoutLastSplit & "|" & textArr(i)
    Next
End Function

'µ¼³ö±íÄÚÐ­ÒéËùÊôÏµÍ³£¬²ÎÊýÎª£º¡¾Ð­ÒéÅäÖÃ±íÂ·¾¶£¬µ¼³ötxtÎÄ¼þÂ·¾¶¡¿
Sub ExportRpcSystemName(excelPath, exportPath)
    Call Plugin.File.DeleteFile(exportPath)
    Dim iPage, iLine, iList, SystemName, NextSystemName
    iPage = 1
    iLine = 6
    iList = 1
    Call Plugin.Office.OpenXls(excelPath)
	
    'Èç¹ûµ±Ç°µ¥Ôª¸ñ¸úÏÂÒ»¸öµ¥Ôª¸ñÄÚµÄÎÄ×Ö²»Ò»Ñù£¬ËµÃ÷ÏµÍ³²»Ò»ÖÂ£¬Ôò½«ÏÂÒ»¸öµ¥Ôª¸ñÐ´Èëtxt£¨¶ÔºóÃæÔÙ´Î³öÏÖÖØ¸´µÄ²»×öÅÐ¶Ï£¬ÔÝÊ±Ö»×ö³õ¼¶´¦Àí£©
    SystemName = ReadFormExcel(iPage, iLine, iList)
    If SystemName <> "" Then 
        Call WriteInTxt(exportPath, SystemName)
        Do
            SystemName = ReadFormExcel(iPage, iLine, iList)
            NextSystemName = ReadFormExcel(iPage, iLine + 1, iList)
            If (SystemName <> NextSystemName) And NextSystemName <> "" Then 
                Call WriteInTxt(exportPath, NextSystemName)
            End If
            If NextSystemName = "" Then 
                Exit Do
            End If
            iLine = iLine + 1
        Loop
    Else 
        MessageBox excelPath & " : ÎÄ¼þ´íÎó£¬Ð­ÒéËùÊôÏµÍ³Îª¿Õ£¡"
    End If
    Call Plugin.Office.CloseXls()
End Sub

'½«Ð­Òé¹¦ÄÜµ¼³öµ½¶ÔÓ¦ÏµÍ³µÄÎÄ¼þ¼ÐÄÚ£¬²ÎÊýÎª£º¡¾Ð­ÒéÅäÖÃ±íÂ·¾¶£¬µ¼³ötxtËùÔÚµÄÄ¿Â¼£¨ÒòÎª»áÓÐ¶à¸ötxtÎÄ¼þ£©¡¿
Sub ExportRpcFuncitonName(excelPath, exportFolderPath)
    Call Plugin.File.DeleteFolder(exportFolderPath)
    Call Plugin.File.CreateFolder(exportFolderPath)

    Dim iPage, iLine, iList, FunctionName, iSystemList
    iPage = 1
    iLine = 6
    iList = 2
    iSystemList = 1
    Call Plugin.Office.OpenXls(excelPath)
    
    FunctionName = ReadFormExcel(iPage, iLine, iList)
    If FunctionName <> "" Then 
        Do
            FunctionName = ReadFormExcel(iPage, iLine, iList)
            If FunctionName = "" Then 
                Exit Do
            End If
            Call WriteInTxt(exportFolderPath & ReadFormExcel(iPage, iLine, iSystemList) & ".txt", FunctionName)
            iLine = iLine + 1
        Loop
    Else 
        MessageBox excelPath & " : ÎÄ¼þ´íÎó£¬Ð­Òé¹¦ÄÜÎª¿Õ"
    End If
    Call Plugin.Office.CloseXls()
End Sub

'½«ÅäÖÃ±íÄÚËùÓÐÄÚÈÝµ¼³ö£¬²ÎÊýÎª£º¡¾Ð­ÒéÅäÖÃ±íÂ·¾¶¡¢µ¼³öÎÄ¼þ¼ÐÂ·¾¶£¨»áÓÐ¶à¸ötxtÎÄ¼þ£©¡¿
Sub ExportAllRpc(excelPath, exportFolderPath)
    Call Plugin.File.DeleteFolder(exportFolderPath)
    Call Plugin.File.CreateFolder(exportFolderPath)
    
    Dim iPage, iLine, iList, inputData, text, systemName
    iPage = 1
    iLine = 6
    iList = 2
    Call Plugin.Office.OpenXls(excelPath)
    
    inputData = ReadFormExcel(iPage, iLine, iList)
    If inputData <> "" Then 
        Do While(ReadFormExcel(iPage, iLine, iList) <> "")
            '½«Í¬Ò»ÐÐµÄÊý¾Ý¶¼ÕûÀí³ÉÒ»¸öÎÄ±¾£¬ÓÃ"|"Á¬½Ó
            //inputData = ReadFormExcel(iPage, iLine, ilist)
            systemName = ReadFormExcel(iPage, iLine, 1)
            inputData = ReadFormExcel(iPage, iLine, iList)
            Do While(ReadFormExcel(iPage, iLine, iList + 1) <> "")
                text = ReadFormExcel(iPage, iLine, ilist + 1)
                inputData = inputData & "|" & text
                iList = iList + 1
            Loop
                    
            'È·¶¨ÎÄ¼þÃû£¬Ð´Èë¶ÔÓ¦µÄÎÄ¼þ
            Call WriteInTxt(exportFolderPath & systemName & ".txt", inputData)
            iLine = iLine + 1
            
            '×îºóÐèÒª½«ÁÐ³õÊ¼»¯£¬ÒòÎªÄÚÑ­»·»á½« ÁÐ µÝÔö£¬Èç¹û²»ÖØÉèÎª0£¬×îÍâ²ãµÄÑ­»·ÅÐ¶Ï¾Í»áÒÔÕâ¸öÖµÎª×¼£¬Ó°ÏìÅÐ¶Ï
            iList = 2
        Loop 
    Else 
        MessageBox excelPath & " : ÎÄ¼þ´íÎó£¬" & iPage & ", " & iLine & ", " & iList & "Êý¾ÝÎª¿Õ"
    End If
 
    Call Plugin.Office.CloseXls()
End Sub


